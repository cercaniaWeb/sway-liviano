    "format": "<span color='#a6e3a1'>ÔÄó </span> {:%H:%M:%S}",
    "format-alt": "<span color='#a6e3a1'>ÔÅ≥ </span> {:%a, %d %b}",
    "interval": 1,
    "tooltip-format": "<tt>{calendar}</tt>",
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#f5e0dc'><b>{}</b></span>",
        "weekdays": "<span color='#f9e2af'><b>{}</b></span>",
        "today": "<span color='#f38ba8'><b>{}</b></span>",
      },
    },
    "actions": {
      "on-click-right": "mode"
    },
    "min-length": 8,
    "max-length": 28,
  },
  "clock#date": {
    // "tooltip": false,
    // "format": "ÔÄó  {:%H:%M:%S}",
    "format": " <span color='#a6e3a1'>ÔÅ≥ </span> {:%A, %d %B}",

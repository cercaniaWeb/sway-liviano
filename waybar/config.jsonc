    ],
    //"thermal-zone": 0,
    "critical-threshold": 82,
    "format-critical": "<span color='#f38ba8'>{temperatureC}¬∞C {icon}</span>",
    "format": "<span color='#a6e3a1'>{icon}</span> {temperatureC}¬∞C",
    "format-icons": [
      "Û∞à∏"
    ],
    "on-click-right": "ghostty -e nvtop",
  },
  // "cpu": {
  //   "format": "Û∞çõ {usage}%",
  //   "format-alt" : "{load}",
  //   "tooltip": false,

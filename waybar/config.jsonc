    },
    "modules": [
      "backlight#icon",
      "backlight#text"
    ],
  },
  "group/battery-group": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 500,
      "children-class": "battery#icon",
      "transition-left-to-right": true,
    },
    "modules": [
      "battery#icon",

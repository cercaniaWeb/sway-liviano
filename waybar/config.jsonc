    "on-click": "sleep 0.1 && swaync-client -t -sw",
    "on-click-right": "swaync-client -d -sw",
    "escape": true,
  },
  // NOTE : Groups from here
  "group/audio": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 500,
